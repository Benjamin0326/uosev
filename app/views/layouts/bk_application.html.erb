<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>TechGut</title>
  <meta name="description" content="">
  <meta name="viewport" content="width=device-width, initial-scale=1">
	
  <!-- Favicon -->
  <link rel="shortcut icon" type="image/png" href="img/favicon.png"/>

  <!-- Fonts -->
  <link href='http://fonts.googleapis.com/css?family=Roboto+Condensed:300italic,400italic,700italic,400,300,700' rel='stylesheet' type='text/css'>
  <link href='http://fonts.googleapis.com/css?family=Open+Sans:400italic,600italic,400,300,600,700' rel='stylesheet' type='text/css'>

  <!-- Revolution css -->
	<%= stylesheet_link_tag    'settings', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    'extralayer', media: 'all', 'data-turbolinks-track' => true %>

 
  <!-- Flat icon css -->
  <%= stylesheet_link_tag    'flaticon', media: 'all', 'data-turbolinks-track' => true %>

  <!-- Font awesome -->
  <%= stylesheet_link_tag    'font-awesome.min', media: 'all', 'data-turbolinks-track' => true %>

  <!-- Owl Carosel css -->
  <%= stylesheet_link_tag    'owl.carousel', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    'owl.theme.default', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    'owl.theme', media: 'all', 'data-turbolinks-track' => true %>

  <!-- mmenu -->
  <%= stylesheet_link_tag    'jquery.mmenu', media: 'all', 'data-turbolinks-track' => true %>

  <!-- Bootstrap css -->
  <%= stylesheet_link_tag    'bootstrap', media: 'all', 'data-turbolinks-track' => true %>
  
  <!-- Animate css-->
  <%= stylesheet_link_tag    'animate', media: 'all', 'data-turbolinks-track' => true %>

  <!-- Custom Style css -->
  <%= stylesheet_link_tag    'hover', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    'style', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    'responsive', media: 'all', 'data-turbolinks-track' => true %>

	<!--[if lt IE 9]>
	<script src="//html5shiv.googlecode.com/svn/trunk/html5.js"></script>
	<script>window.html5 || document.write('<script src="js/vendor/html5shiv.js"><\/script>')
	</script>
	<![endif]-->

	
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= yield %>

</body>
</html>
